<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>MPlayer</display-name>
  <welcome-file-list>
    <welcome-file>mplayer.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>gp</servlet-name>
    <servlet-class>com.mplayer.Playlist</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>gp</servlet-name>
    <url-pattern>/gp</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>To get playlist</description>
    <display-name>Playlist</display-name>
    <servlet-name>Playlist</servlet-name>
    <servlet-class>com.mplayer.Playlist</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Playlist</servlet-name>
    <url-pattern>/Playlist</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>Servlet to search results</description>
    <display-name>SearchResults</display-name>
    <servlet-name>SearchResults</servlet-name>
    <servlet-class>com.mplayer.SearchResults</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SearchResults</servlet-name>
    <url-pattern>/SearchResults</url-pattern>
  </servlet-mapping>
</web-app>